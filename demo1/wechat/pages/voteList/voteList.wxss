/* pages/voteList/voteList.wxss */
@import "../templates/votelist2/votelist2.wxss";
 .lunbo {
  width: 100%;
  height: 40vw;
  display: block;
  background: #f0f0f0;
  z-index: 999;
}
.lunbo-img {
  width: 100%;
  height: inherit;
  /* border-radius: 0 0 30rpx 30rpx; */
}
.voteList{
  display: flex;
  flex-wrap: wrap;
  justify-content: center;
  padding-bottom: 10rpx;
  z-index: 666;
}
.dixian{
  background-color: #fff;
}
.background_view{
  width: 100vw;
  height: 20vw;
  position: fixed;
  top: 0;
  left: 0;
  background: -webkit-gradient(linear, 0 0, 0 100%, from(#336799), to(#12cadb));
  padding: 24rpx 0;
  z-index: -99;
  border-radius: 0 0 80% 80%;
}